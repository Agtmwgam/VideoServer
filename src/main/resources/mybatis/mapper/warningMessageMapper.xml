<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tw.dao.WarningMessageDao">


    <!-- 告警数据入库 -->
    <insert id="saveWarningMessage" parameterType="com.tw.entity.WarningMessage">
        insert into warningMessage
        (frame,mesNo,mesDate,groupName,deviceName,deviceModel,serial,videoResolution,targetLocation,exeStatus,ip)
        values(#{frame},#{mesNo},#{mesDate},#{groupName},#{deviceName},#{deviceModel},#{serial},#{videoResolution},#{targetLocation},#{exeStatus},#{ip})
    </insert>



</mapper>